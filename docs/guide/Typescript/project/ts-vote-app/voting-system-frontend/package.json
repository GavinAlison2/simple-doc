{
    "name": "voting-system-frontend",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "axios": "^0.24.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.3.0",
        "react-scripts": "4.0.3",
        "typescript": "^4.4.3",
        "voting-system-frontend": "file:"
    },
    "scripts": {
        "start": "set BROWSER=none && set NODE_OPTIONS=--openssl-legacy-provider &&  react-scripts start",
        "start:watch": "set BROWSER=none && set NODE_OPTIONS=--openssl-legacy-provider &&  nodemon  --watch src --watch public --exec react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "clean": "rm -rf  node_modules && rm -rf package-lock.json"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 Safari version"
        ]
    },
    "devDependencies": {
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.2",
        "@types/react-router-dom": "^5.3.3",
        "nodemon": "^3.1.9"
    }
}